Group gHue "Hue"
Group gBulb1 "Bulb 1" (gHue)
Group gBulb2 "Bulb 2" (gHue)
Group gIris1 "Iris 1" (gHue)


/* Bulb 1*/
Switch  hue_bulb210_light_1      "On/Off"               (gBulb1)            {channel="hue:0210:hue_1:bulb1:color"}
Dimmer hue_bulb210_dimmer_1      "Dimmer"               (gBulb1)            {channel="hue:0210:hue_1:bulb1:color"}
Color  hue_bulb210_color_1       "Color"                (gBulb1)            {channel="hue:0210:hue_1:bulb1:color"}
Dimmer hue_bulb210_colorTemp_1   "Color-Temperature"    (gBulb1)            {channel="hue:0210:hue_1:bulb1:color_temperature"}
/*supported values NONE, SELECT, LSELECT*/
String  hue_bulb210_alert_1      "Alert"                (gBulb1)           {channel="hue:0210:hue_1:bulb1:alert"}
Switch  hue_bulb210_effect_1     "Effect"               (gBulb1)           {channel="hue:0210:hue_1:bulb1:effect"}

/* Bulb 2*/
Switch  hue_bulb210_light_2      "On/Off"               (gBulb2)            {channel="hue:0210:hue_1:bulb2:color"}
