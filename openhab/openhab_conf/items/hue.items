Color  hue_bulb210_color_2       "Color"                (gBulb2)            {channel="hue:0210:hue_1:bulb2:color"}
Dimmer hue_bulb210_colorTemp_2   "Color-Temperature"    (gBulb2)            {channel="hue:0210:hue_1:bulb2:color_temperature"}
/*supported values NONE, SELECT, LSELECT*/
String  hue_bulb210_alert_2      "Alert"                (gBulb2)            {channel="hue:0210:hue_1:bulb2:alert"}
Switch  hue_bulb210_effect_2     "Effect"               (gBulb2)            {channel="hue:0210:hue_1:bulb2:effect"}

/* Iris*/
Switch  hue_iris_light_1         "On/Off"               (gIris1)            {channel="hue:0200:hue_1:iris1:color"}
Dimmer hue_iris_dimmer_1         "Dimmer"               (gIris1)            {channel="hue:0200:hue_1:iris1:color"}
